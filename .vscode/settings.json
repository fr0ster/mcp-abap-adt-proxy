{
    // Restricts spell checking to files inside the project only
    "cSpell.files": [
      "**/*"
    ],
    "cSpell.ignorePaths": [
      // Ignore everything outside the project's root folder
      "!**"
    ],
    "cSpell.words": [
        "dedupe",
        "EADDRINUSE",
        "findstr",
        "loglevel",
        "retryable",
        "softprops",
        "streamable",
        "USERPROFILE"
    ]
}