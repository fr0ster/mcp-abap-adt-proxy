{
    // Restricts spell checking to files inside the project only
    "cSpell.files": [
      "**/*"
    ],
    "cSpell.ignorePaths": [
      // Ignore everything outside the project's root folder
      "!**"
    ],
    "cSpell.words": [
      "chrono",
      "dedupe",
      "EADDRINUSE",
      "findstr",
      "loglevel",
      "retryable",
      "softprops",
      "streamable",
      "USERPROFILE"
    ]
}